Title: Le format de fichier Safetensors
Version: 0.1.2.9000
Date: 2024-05-26
Authors@R: c(
    person(given = "Christophe",
           family = "Regouby",
           role = c("aut", "cre", "cph"),
           email = "christophe.regouby@free.fr"
           )
